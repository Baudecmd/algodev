				<Button id="record-sales" layoutX="438.0" layoutY="200.0"
					mnemonicParsing="false" text="Poker" onAction="#buttonPoker" />
				<Button id="record-sales" layoutX="272.0" layoutY="200.0"
					mnemonicParsing="false" text="Bataille Navale "
